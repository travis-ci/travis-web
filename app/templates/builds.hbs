{{#if model.isLoaded}}
  <ul class="build-list">
    {{#each builds as |build|}}
      {{builds-item build=build}}
    {{else}}
      {{no-builds}}
    {{/each}}
  </ul>
  {{#if displayShowMoreButton}}
    <p>
      {{#if loadMoreTask.isRunning}}
        {{loading-indicator}}
      {{else}}
        {{show-more-button showMore=(action 'showMoreBuilds')}}
      {{/if}}
    </p>
  {{/if}}

{{else}}
  {{loading-indicator}}
{{/if}}
