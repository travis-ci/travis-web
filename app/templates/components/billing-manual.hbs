{{#unless this.subscription.manualSubscriptionExpired}}
  <section>
    <h3>
      Overview
    </h3>
    <h3 data-test-plan-name>
      {{this.subscription.plan.name}}
    </h3>
    <p data-test-plan-concurrency>
      {{pluralize this.subscription.plan.builds "concurrent job"}}
    </p>
    <p>
      Renewing on {{moment-format this.subscription.validTo "MMMM D, YYYY"}}
    </p>
  </section>
  <section>
    <h3>
      Billing Information
    </h3>
    <section class="row">
      <section class="payment">
        <h3>
          Payment details
        </h3>
        <Billing::PaymentDetails @subscription={{this.subscription}} />
      </section>
    </section>
  </section>
{{else}}
  <span data-test-expiry-message>
    {{#if this.subscription.manualSubscriptionExpired}}
      You had a {{this.subscription.sourceWords}} subscription that expired on
      {{moment-format this.subscription.validTo "MMMM D, YYYY"}}.
      <ManualSubscriptionHelp />
    {{/if}}
  </span>
{{/unless}}
