<div class="missing-notice">
  <figure class="page-graphic">
    <SvgImage @name="barricade" @class="barricade" />
  </figure>
  <h2 class="page-title">
    We couldn't find the repository<br />
    <span class="h2--red">
      {{this.slug}}
    </span>
  </h2>
  {{#if this.features.proVersion}}
    {{#unless this.auth.signedIn}}
      <p class="page-notice">
        This repository may not exist or you may need to
        <LinkTo @route="signin" class="auth-button--white m-l-s">
          Sign in
        </LinkTo>
      </p>
    {{/unless}}
  {{/if}}
</div>
