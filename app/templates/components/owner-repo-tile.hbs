{{!-- {{#if repo.private }}
         <div class="tile-lock"><span class="icon icon-lock"></span></div>
         {{/if}} --}}
<div class="row-item fade-out">
  <div class="one-line">
    <h2 class="repo-title">
      {{#link-to "repo" ownerName repoName class="label-align"}}
        {{status-icon status=repo.default_branch.last_build.state}}
        <span class="inner-underline">{{repoName}}</span>
      {{/link-to}}
    </h2>
  </div>
</div>
{{#if repo.default_branch.last_build}}
  <div class="row-item row-color build-number">
    <div class="one-line">
      {{#link-to "build" ownerName repoName repo.default_branch.last_build.id}}
        {{svg-jar 'icon-hash' class="icon"}}
        <span class="label-align inner-underline">{{repo.default_branch.last_build.number}}</span>
      {{/link-to}}
    </div>
  </div>

  <div class="row-item fade-out default-branch">
    <div class="one-line">
      {{request-icon event=repo.default_branch.last_build.event_type}}
      <span class="label-align">{{repo.default_branch.name}}</span>
    </div>
  </div>

  <div class="row-item commit-sha">
    {{tooltip-on-element text='View diff on GitHub'}}
    {{#external-link-to href=repo.default_branch.last_build.commit.compare_url}}
      {{svg-jar 'icon-compare' class="icon"}}
      <span class="label-align inner-underline commit-compare">
        {{format-sha repo.default_branch.last_build.commit.sha}}
      </span>
      {{svg-jar 'icon-external-link' class="icon"}}
    {{/external-link-to}}
  </div>

  <div class="row-item fade-out commit-date">
    <div class="one-line">
      {{svg-jar 'icon-calendar' class="icon"}}
      <span class="build-status label-align">{{repo.default_branch.last_build.state}}</span>
      <span class="finished-at label-align">{{format-time repo.default_branch.last_build.finished_at}}</span>
    </div>
  </div>
{{else}}
  <p class="row-item">There is no build on the default branch yet.</p>
{{/if}}
