{{#if account.hasSubscriptionPermissions}}
<a href={{subscription.billingUrl}} class='button--green manage-subscription resubscribe'>
  Resubscribe
</a>
{{else}}
<div class='button--green manage-subscription disabled resubscribe'>
  Resubscribe{{tooltip-on-element text='You do not have permission to resubscribe'}}
</div>
{{/if}}
