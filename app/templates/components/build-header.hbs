<div class="build-header-container">
  <div class="build-commit">
    <h2 class="build-title {{item.state}}">
      <StatusIcon @status={{item.state}} />
      {{#if item.isPullRequest}}
        <small class="commit-branch" title={{item.pullRequestTitle}}>
          Pull Request #
          {{item.pullRequestNumber}}
        </small>
        {{#if build.request.isDraft}}
          <span data-test-build-header-draft-badge class="message-label badge">
            draft
          </span>
        {{/if}}
        {{item.pullRequestTitle}}
      {{else}}
        {{#if item.isTag}}
          <small class="commit-branch" title={{item.tag.name}}>
            {{item.tag.name}}
          </small>
        {{else}}
          <small class="commit-branch" title={{item.branch.name}}>
            {{item.branch.name}}
          </small>
        {{/if}}
        {{format-message
          commit.subject
          repo=item.repo
          eventType=build.eventType
          maxLength=60
        }}
      {{/if}}
    </h2>
    <div class="commit-info">
      {{#if commit.body}}
        <p class="commit-description" title="{{commit.body}}">
          <span class="monospace {{commitBody}}">
            {{format-message commit.body repo=item.repo pre=true}}
          </span>
        </p>
      {{/if}}
      <ul class="list-icon">
        <li>
          <ExternalLinkTo @href={{commit.githubUrl}}>
            <EmberTooltip @text="View commit on GitHub" />
            {{svg-jar "icon-commit" class="icon"}}
            <span class="label-align inner-underline commit-commit">
              Commit
              <span class="label-align-item monospace">
                {{format-sha commit.sha}}
              </span>
            </span>
            {{svg-jar "icon-external-link" class="icon"}}
          </ExternalLinkTo>
        </li>
        {{#if displayCompare}}
          <li>
            {{#if item.isPullRequest}}
              <ExternalLinkTo @href={{item.commit.compareUrl}}>
                <EmberTooltip @text="View Pull Request on GitHub" />
                {{svg-jar "icon-compare" class="icon"}}
                <span class="label-align inner-underline commit-compare">
                  #
                  {{item.pullRequestNumber}}
                  :
                  {{item.pullRequestTitle}}
                </span>
                {{svg-jar "icon-external-link" class="icon"}}
              </ExternalLinkTo>
            {{else if item.commit.compareUrl}}
              <ExternalLinkTo @href={{item.commit.compareUrl}}>
                <EmberTooltip @text="View diff on GitHub" />
                {{svg-jar "icon-compare" class="icon"}}
                <span class="label-align inner-underline commit-compare">
                  Compare
                  <span class="label-align-item monospace">
                    {{short-compare-shas item.commit.compareUrl}}
                  </span>
                </span>
                {{svg-jar "icon-external-link" class="icon"}}
              </ExternalLinkTo>
            {{/if}}
          </li>
        {{/if}}
        <li>
          {{#if item.isTag}}
            <ExternalLinkTo @href={{urlGitHubTag}} class="commit-branch-url">
              <EmberTooltip @text="View tag on GitHub" />
              {{svg-jar "icon-tag" class="icon"}}
              <span class="label-align inner-underline">
                Tag
                {{build.tag.name}}
              </span>
              {{svg-jar "icon-external-link" class="icon"}}
            </ExternalLinkTo>
          {{else}}
            <ExternalLinkTo @href={{urlGitHubBranch}} class="commit-branch-url">
              <EmberTooltip @text="View branch on GitHub" />
              {{svg-jar "icon-branch" class="icon"}}
              <span class="label-align inner-underline">
                Branch
                <span class="label-align-item monospace">
                  {{build.branchName}}
                </span>
              </span>
              {{svg-jar "icon-external-link" class="icon"}}
            </ExternalLinkTo>
          {{/if}}
        </li>
      </ul>
      <p class="commit-author">
        <CreatedBy
          @user={{build.createdBy}}
          @commit={{commit}}
          @eventType={{build.eventType}}
        />
      </p>
    </div>
  </div>
  <div class="build-info">
    <h3 class="build-status {{item.state}}">
      {{#if isJob}}
        <LinkTo @route="job" @models={{array repo item}}>
          {{#if item.isTag}}
            {{svg-jar "icon-tag" class=(concat item.state " icon")}}
          {{else}}
            <RequestIcon @event={{item.eventType}} @state={{item.state}} />
          {{/if}}
          <span data-test-build-header-build-state class="inner-underline">
            #
            {{item.number}}
            {{humanize-state item.state}}
          </span>
        </LinkTo>
      {{else}}
        <LinkTo @route="build" @models={{array repo item}}>
          {{#if item.isTag}}
            {{svg-jar "icon-tag" class=(concat item.state " icon")}}
          {{else}}
            <RequestIcon @event={{item.eventType}} @state={{item.state}} />
          {{/if}}
          <span data-test-build-header-build-state class="inner-underline">
            #
            {{item.number}}
            {{humanize-state buildState}}
          </span>
        </LinkTo>
      {{/if}}
    </h3>
    <ul class="list-icon">
      {{#if isJob}}
        <li class="commit-stopwatch">
          {{svg-jar "icon-stopwatch" class="icon--m"}}
          <span class="label-align">
            {{#if item.toBeQueued}}
              Waiting to be queued
            {{else if item.toBeStarted}}
              Waiting to start
            {{else}}
              {{if item.isFinished "Ran for" "Running for"}}
              <time datetime="PT{{item.elapsedTime}}S">
                {{format-duration item.elapsedTime}}
              </time>
            {{/if}}
          </span>
        </li>
      {{else}}
        <li
          title="{{if
              item.elapsedTime
              (concat "Started " (pretty-date item.startedAt))
            }}"
          class="commit-stopwatch"
        >
          {{svg-jar "icon-stopwatch" class="icon--m"}}
          <span class="label-align">
            {{if item.isFinished "Ran for" "Running for"}}
            <time datetime="PT{{item.elapsedTime}}S">
              {{format-duration item.elapsedTime}}
            </time>
          </span>
        </li>
      {{/if}}
      {{#if item.isFinished}}
        {{#if item.isMatrix}}
          <li class="commit-clock">
            {{svg-jar "icon-clock" class="icon"}}
            <span class="label-align">
              Total time
              <time datetime="PT{{item.duration}}S">
                {{format-duration item.duration}}
              </time>
            </span>
          </li>
        {{/if}}
        <li
          title="Finished {{pretty-date item.finishedAt}}"
          class="commit-calendar"
        >
          {{svg-jar "icon-calendar" class="icon"}}
          <time class="label-align" datetime={{pretty-date item.finishedAt}}>
            {{format-time item.finishedAt}}
          </time>
        </li>
      {{/if}}
    </ul>
  </div>
  <div class="build-tools">
    {{#if isJob}}
      <RepoActions @job={{item}} @repo={{item.repo}} />
    {{else}}
      <RepoActions @build={{item}} @repo={{item.repo}} />
    {{/if}}
  </div>
</div>
{{#if isNotMatrix}}
  <div class="job-matrix-name-separator"></div>
  <div class="job-container">
    <div class="job-detail-container">
      <div class="detail-job-os {{os}}">
        <EmberTooltip @text={{os}} />
        {{svg-jar osIcon class="icon"}}
      </div>
      {{#if name}}
        <div class="detail-job-name">
          {{svg-jar "job-name-icon" class="icon"}}
          <span class="label-align" aria-label="Title">
            {{name}}
          </span>
        </div>
      {{/if}}
      <div class="detail-job-lang">
        {{svg-jar "icon-language" class="icon"}}
        <span class="label-align" aria-label="Language">
          {{#if languages}}
            {{languages}}
          {{else}}
            no language set
          {{/if}}
        </span>
      </div>
    </div>
    {{#if environment}}
      <div
        class="detail-job-env"
        {{action "expandEnv" on="click"}}
        {{action "expandEnv" on="mouseEnter"}}
        {{action "expandEnv" on="mouseLeave"}}
      >
        {{svg-jar "icon-environment" class="icon"}}
        <span class="label-align" aria-label="Environment variables">
          {{environment}}
        </span>
      </div>
    {{/if}}
  </div>
{{/if}}