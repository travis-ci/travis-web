{{#if isEmpty}}
  {{svg-jar 'icon-nobuilds'}}
{{else}}
  {{#if hasPassed}}
    {{svg-jar 'icon-passed'}}
  {{/if}}

  {{#if hasFailed}}
    {{svg-jar 'icon-failed'}}
  {{/if}}

  {{#if wasCanceled}}
    {{svg-jar 'icon-canceled'}}
  {{/if}}

  {{#if hasErrored}}
    {{svg-jar 'icon-errored'}}
  {{/if}}

  {{#if isRunning}}
    {{svg-jar 'icon-running'}}
  {{/if}}
{{/if}}
