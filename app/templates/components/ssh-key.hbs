<div class="ssh-key-name">
  {{svg-jar "icon-key" class="icon"}}
  <span>
    {{if key.isCustom key.description "Default"}}
  </span>
</div>
<div class="ssh-key-value">
  {{svg-jar "icon-fingerprint" class="icon"}}
  <span>
    {{key.fingerprint}}
  </span>
</div>
{{#if key.isCustom}}
  <div class="ssh-key-action">
    {{#if delete.isRunning}}
      <LoadingIndicator />
    {{else if pushAccess}}
      <button
        type="button"
        class="no-button ssh-delete"
        {{action (perform delete)}}
      >
        {{svg-jar "icon-trash" class="icon"}}
      </button>
      <EmberTooltip @text="Delete" />
    {{else}}
      {{svg-jar "icon-trash-disabled" class="icon ssh-no-delete"}}
      <EmberTooltip @text="You canâ€™t delete keys" />
    {{/if}}
  </div>
{{else}}
  <div class="ssh-key-action">
    {{svg-jar "icon-trash-disabled" class="icon ssh-no-delete"}}
    <EmberTooltip @text="Default key cannot be deleted" />
  </div>
{{/if}}