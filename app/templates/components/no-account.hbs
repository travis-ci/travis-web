<div class="missing-notice">
  <figure class="page-graphic">
    <SvgImage @name="barricade" />
  </figure>
  <h2 class="page-title no-account">
    We couldn't find the {{vcs-vocab this.vcsType 'organization' lower=true}}<br />
    <span class="h2--red">
      {{this.name}}
    </span>
  </h2>
  <p class="helptext">
    The {{vcs-vocab this.vcsType 'organization' lower=true}} may not exist or you may not have access to it.
  </p>
</div>
