<UiKit::Box
  @tag={{this.tag}}
  @borderColor={{this.borderColor}}
  @borderWidth={{this.borderWidth}}
  @display={{this.display}}
  @flex={{this.flex}}
  @margin={{this.margin}}
  @maxWidth={{this.maxWidth}}
  @padding={{this.padding}}
  class={{this.allClasses}}
  ...attributes
>
  {{~yield~}}
</UiKit::Box>