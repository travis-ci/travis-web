<div class="row-item row-branch">
  <RequestIcon @event={{cache.type}} />
  <span class="label-align">
    {{cache.branch}}
  </span>
</div>
<div class="row-item row-calendar">
  {{svg-jar "icon-calendar" class="icon"}}
  <span class="label-align">
    {{format-time cache.last_modified}}
  </span>
</div>
<div class="row-item row-size">
  {{svg-jar "icon-scales" class="icon"}}
  <span class="label-align">
    {{travis-mb cache.size}}
    MB
  </span>
</div>
<div class="row-item">
  <a
    href="#"
    class="{{if delete.isRunning "deleting"}} delete-cache-icon"
    title="Delete this cache"
    {{action "performDelete"}}
  >
    <EmberTooltip @text="Delete" />
    {{svg-jar "icon-trash" class="icon"}}
  </a>
</div>