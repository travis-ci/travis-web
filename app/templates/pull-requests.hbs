{{#if isLoaded}}
  <ul class="build-list">
    {{#each builds as |build|}}
      {{builds-item build=build}}
    {{else}}
      {{missing-notice title="No pull request builds for this repository"}}
    {{/each}}
  </ul>
  {{#if displayShowMoreButton}}
    <p>
      {{show-more-button isLoading=isLoading showMore=(action 'showMoreBuilds')}}
      {{#if isLoading}}
        {{loading-indicator}}
      {{/if}}
    </p>
  {{/if}}
{{else}}
  {{loading-indicator}}
{{/if}}
