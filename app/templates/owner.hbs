{{#travis-layout layoutName="layouts/center" class="owner"}}
<header class="owner-header row">
  <div class="owner-avatar">
    {{user-avatar url=owner.avatar name=owner.name size=126}}
  </div>
  <div class="owner-info">
    <h1 class="owner-title">{{owner.name}}</h1>
    <p class="owner-handle">
      {{svg-jar 'icon-repooctocat' class="icon--l"}}
      <a href={{githubProfile}} title="{{owner.name}} on GitHub">
        <span class="label-align">{{owner.login}}</span></a></p>
  </div>
</header>

{{owner-tabs data-test-owner-tabs login=owner.login}}
{{outlet}}
{{/travis-layout}}
