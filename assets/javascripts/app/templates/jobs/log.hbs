{{#if log.isLoaded}}
  <pre id="log">{{{formatLog log.body}}}</pre>

  {{#if sponsor.name}}
    <p class="sponsor">
    {{t builds.messages.sponsored_by}}
      <a {{bindAttr href="sponsor.url"}}>{{sponsor.name}}</a>
    </p>
  {{/if}}
{{else}}
  <div id="log" class="loading">
    <span>Loading</span>
  </div>
{{/if}}
