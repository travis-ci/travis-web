{
  "framework": "qunit",
  "test_page": "tests/index.html?hidepassed",
  "launch_in_ci": [
    "SL_chrome",
    "SL_firefox",
    "PhantomJS"
  ],
  "launch_in_dev": [
    "PhantomJS"
  ],
  "launchers": {
    "SL_chrome": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "chrome",
        "-p",
        "Windows 8",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_firefox": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "firefox",
        "-p",
        "Windows 8",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_safari": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "safari",
        "-p",
        "Mac 10.10",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    }
  }
}
